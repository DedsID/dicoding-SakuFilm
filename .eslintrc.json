{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "linebreak-style": ["error", "windows"],
        "no-console": ["error", { "allow": ["warn", "error", "log"] }],
        "import/extensions": [<severity>, "never" , "always" , "ignorePackages"]
    }
}
